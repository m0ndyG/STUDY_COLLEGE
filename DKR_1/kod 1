program zadanie1;
var
  x, res: Real;
begin
writeln('Введите x: ');
readln(x);
    if x < -8 then
      res := ln(abs(x)) * cos(x) + sqr(x) / sin(x)
    else if (x >= -8) and (x < -4) then
      res := 1 / cos(2 * x) * sin(x) / x
    else if (x >= -4) and (x < 3) then
      res := sqr(x) / exp(1/3 * ln(x)) + x
    else
      res := cos(x) / sqr(x);

    writeln('f (', x:0:2, ') = ', res:0:2);

end.
